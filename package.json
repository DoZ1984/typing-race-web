{
  "name": "typing-race-web",
  "version": "1.0.0",
  "description": "Aplicación web de carreras de mecanografía con gamificación y torneos",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "server:dev": "cd server && npm run dev",
    "install": "concurrently \"cd server && npm install\" \"cd client && npm install\""
  },
  "keywords": ["mecanografía", "carreras", "typing", "juego", "multijugador", "torneos"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}